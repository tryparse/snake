<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
    <appSettings>
        <add key="IsLoggingEnabled" value="false"/>
        <add key="IsRenderingEnabled" value="true"/>
        <add key="IsDebugRenderingEnabled" value="true"/>
        <add key="ScreenWidth" value="800"/>
        <add key="ScreenHeight" value="600"/>
        <add key="IsFullScreen" value="false"/>
        <add key="TileSize" value="40"/>
        <add key="MapWidth" value="10"/>
        <add key="MapHeight" value="10"/>
        <add key="DefaultMoveIntervalTime" value="800"/>
        <add key="LimitMoveIntervalTime" value="150"/>
        <add key="RemainingLives" value="3"/>
    </appSettings>
</configuration>
